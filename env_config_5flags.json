{
    "jdk_path": "jdk-11.0.22.jdk",
    "bm_path": "dacapo-bench.jar",
    "bm_name": "avrora",
    "gc_viewer_jar": "gcviewer-1.36.jar",
    "callback_path": "callback/VMStatCallback.java",
    "n": 5,
    "verbose": false,
    "beta": 0.0,
    "default_flags": ["-XX:+UseParallelGC"],
    "run_offline": true,
    "goals": 
        [
            {
                "name": "throughput",
                "minimize": false
            }
        ],
    "flags":
        [
            {
                "name": "MaxTenuringThreshold",
                "min": 1, 
                "max": 16,
                "step": 3,
                "unit": ""
            },
            {
                "name": "ParallelGCThreads",
                "min": 4, 
                "max": 24,
                "step": 4,
                "unit": ""
            },
            {
                "name": "MaxHeapSize",
                "min": 2, 
                "max": 24,
                "step": 4,
                "unit": "G"
            },
            {
                "name": "SurvivorRatio",
                "min": 8, 
                "max": 16,
                "step": 1,
                "unit": ""
            },
            {
                "name": "TargetSurvivorRatio",
                "min": 50, 
                "max": 90,
                "step": 1,
                "unit": ""
            }
        ],

    "params": 
    [
        "fullGCPause", 
        "avgPause", 
        "fullGcPauseCount", 
        "footprint", 
        "gcPerformance",
        "totalTenuredUsedMax", 
        "avgPromotion", 
        "fullGCPerformance"
    ]
}